

public class Program {
	
	//1. 컴파일 -> Exam.class
	//2. 압축 -> Exam.zip
	//3. jar -> Exam.jar (java archive) 
	
	public static void main(String[] args) {
		NewlecExam exam = new NewlecExam();
		exam.setMath(20); 
		exam.setCom(20);
		System.out.println(exam.total());
		System.out.println(exam.getCom());
		
		
		
	}

}
