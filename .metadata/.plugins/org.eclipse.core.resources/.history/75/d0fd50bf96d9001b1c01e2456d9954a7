package part2.ex3.데이터구조화;

public class ExamProgram {

	public static void main(String[] args) {
		Exam exam = new Exam(); // class Exam 참조하는 exam. 새로 만들어진 객체를 식별하는 이름.
		exam.kor = 30;
		inputExam(exam);
		
		System.out.printf("Korea : %d, English : %d, Math : %d", exam.kor,exam.eng,exam.math);

	}

	private static void inputExam(Exam exam) {
		exam.eng = 50;
		exam.math = 30;
	}

}
