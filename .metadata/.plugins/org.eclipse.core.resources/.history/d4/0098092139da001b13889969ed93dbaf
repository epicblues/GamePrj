package part3.ex3.Gettersì™€Setters;

public class Exam {
	private int kor;
	private int eng;
	private int math;
	
	public int getKor() {
		return kor;
	}
	public int getEng() {
		return eng;
	}
	public int getMath() {
		return math;
	}
	public void setKor(int kor2) {
		kor = kor2;
		
	}
	public void setEng(int eng2) {
		eng = eng2;
		
	}
	public void setMath(int math2) {
		math = math2;
		
	}
}
