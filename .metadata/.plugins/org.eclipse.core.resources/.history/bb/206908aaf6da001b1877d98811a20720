 package part3.ex5.추상화;

public class NewlecExam extends Exam{

	private int com;
	
	public NewlecExam() {
		this(0,0,0,0);
	}
	
	public NewlecExam(int kor, int eng, int math, int com) {
		super(kor,eng,math);
		this.com = com;
	}
	
	@Override
	public int total() {
		
		return getKor() + getEng() + getMath() + com;
	}

	@Override
	public float avg() {
		
		return 0;
	}
	
}
